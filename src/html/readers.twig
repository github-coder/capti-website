<script>
	function labnolThumb(e){return'<img class="youtube-thumb" src="//i.ytimg.com/vi/'+e+'/hqdefault.jpg"><div class="play-button"></div>'}function labnolIframe(){var e=document.createElement("iframe");e.setAttribute("src","//www.youtube.com/embed/"+this.parentNode.dataset.id+"?autoplay=1&autohide=2&border=0&wmode=opaque&enablejsapi=1&showinfo=0"),e.setAttribute("frameborder","0"),e.setAttribute("id","youtube-iframe"),e.setAttribute("allowfullscreen",""),this.parentNode.replaceChild(e,this)}for(var v=document.getElementsByClassName("youtube-player"),n=0;n<v.length;n++){var p=document.createElement("div");p.innerHTML=labnolThumb(v[n].dataset.id),p.onclick=labnolIframe,v[n].appendChild(p)}
